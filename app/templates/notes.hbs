<<div id="container">

<p>
  {{message}}
</p>

<ul>
{{#each note in model}}
  <li>{{note.title}}  {{note.body}} <button {{action "deleteNote" note}}>Delete</button></li>

{{/each}}
</ul>
<p>{{input value =newTitle placeholder="Add a title"}}</p>

<p>{{textarea value=newBody cols="50" rows="10" placeholder="Add a note"}}</p>

<p>
  <div class="ui button">
  <button {{action "create"}}>Add Note</button>
</div>
</p>
</div>
